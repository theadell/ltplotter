{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ExprPlotRequest",
    "definitions": {
        "ExprPlotRequest": {
            "properties": {
                "x_label": {
                    "type": "string"
                },
                "y_label": {
                    "type": "string"
                },
                "x_min": {
                    "type": "integer"
                },
                "x_max": {
                    "type": "integer"
                },
                "y_min": {
                    "type": "integer"
                },
                "y_max": {
                    "type": "integer"
                },
                "axis_lines": {
                    "enum": [
                        "AXIS_LINES_BOX",
                        0,
                        "AXIS_LINES_LEFT",
                        1,
                        "AXIS_LINES_RIGHT",
                        2,
                        "AXIS_LINES_MIDDLE",
                        3,
                        "AXIS_LINES_CENTER",
                        4,
                        "AXIS_LINES_NONE",
                        5
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Axis Lines"
                },
                "border": {
                    "type": "integer"
                },
                "grid": {
                    "type": "boolean"
                },
                "title": {
                    "type": "string"
                },
                "plots": {
                    "items": {
                        "$ref": "#/definitions/plot.PlotExpression"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Expr Plot Request"
        },
        "plot.PlotExpression": {
            "properties": {
                "expression": {
                    "type": "string"
                },
                "domain": {
                    "type": "string"
                },
                "samples": {
                    "type": "integer"
                },
                "color": {
                    "type": "string"
                },
                "line_style": {
                    "type": "string"
                },
                "line_width": {
                    "type": "string"
                },
                "legend": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Plot Expression"
        }
    }
}